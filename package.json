{
  "name": "tinygems",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tscheck": "tsc --noEmit",
    "docker:up": "docker run -d -p 27017:27017 --name tinygems-mongodb mongo:latest",
    "docker:down": "docker stop tinygems-mongodb && docker rm tinygems-mongodb",
    "docker:status": "docker ps --filter name=tinygems-mongodb",
    "predev": "docker ps --filter name=tinygems-mongodb -q ||  bun run docker:up"
  },
